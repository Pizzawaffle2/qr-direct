{
  "name": "qr-direct",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "fix": "tsx scripts/fix-eslint-errors.ts && tsx scripts/fix-quotes.ts && eslint . --fix",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "eslint --max-warnings=0 src",
    "format": "prettier --write 'src/**/*.{ts,tsx,css,md,json}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,css,md,json}'",
    "check-types": "tsc --noEmit",
    "validate": "npm run lint && npm run check-types",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "dev": "cross-env NODE_ENV=development next dev",
    "build": "next build",
    "start": "next start",
    "clean": "rimraf .next out",
    "predev": "npm run clean",
    "prebuild": "npm run clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "db:migrate": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:init": "tsx scripts/db/init.ts",
    "db:reset": "tsx scripts/db/reset.ts",
    "db:seed": "tsx scripts/db/seed.ts",
    "db:setup": "prisma generate && prisma db push && npm run db:init && npm run db:seed",
    "build:prod": "cross-env NODE_ENV=production next build && node scripts/cleanup.js",
    "start:prod": "cross-env NODE_ENV=production next start",
    "analyze": "cross-env ANALYZE=true next build",
    "postinstall": "prisma generate",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:shell": "docker-compose exec app sh",
    "docker:db:push": "docker-compose exec app npx prisma db push",
    "docker:db:generate": "docker-compose exec app npx prisma generate",
    "docker:db:studio": "docker-compose exec app npx prisma studio",
    "docker:db:reset": "docker-compose exec app npx prisma migrate reset --force",
    "check:unused": "ts-node scripts/analyze-build.ts",
    "update:deps": "npm-check-updates -u && npm install"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.4",
    "@aws-sdk/client-secrets-manager": "^3.693.0",
    "@aws-sdk/client-ssm": "^3.693.0",
    "@hookform/resolvers": "^3.9.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-alert-dialog": "^1.1.2",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-context-menu": "^2.2.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-hover-card": "^1.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.1",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "@react-email/components": "^0.0.28",
    "@stripe/stripe-js": "^2.3.0",
    "@tsparticles/react": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@upstash/ratelimit": "^1.0.0",
    "@upstash/redis": "^1.28.0",
    "canvas-confetti": "^1.9.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.11.17",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^2.5.2",
    "lucide-react": "^0.460.0",
    "next": "^15.0.3",
    "next-auth": "^4.24.7",
    "next-themes": "^0.4.3",
    "nodemailer": "^6.9.16",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.53.2",
    "react-icons": "^5.3.0",
    "react-scroll-parallax": "^3.4.5",
    "recharts": "^2.13.3",
    "resend": "^2.1.0",
    "stripe": "^14.11.0",
    "tailwind-merge": "^2.5.4",
    "tsparticles": "^3.6.0",
    "zod": "^3.23.8",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^0.8.0",
    "@aws-amplify/backend-cli": "^0.9.0",
    "@next/bundle-analyzer": "^15.0.3",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/classnames": "^2.3.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.6",
    "@types/nodemailer": "^6.4.16",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "autoprefixer": "^10.4.20",
    "aws-cdk": "2.110.1",
    "aws-cdk-lib": "2.110.1",
    "bcryptjs": "^2.4.3",
    "constructs": "^10.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.18",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "filesize": "^10.1.6",
    "glob": "^11.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.10",
    "next-pwa": "^5.6.0",
    "npm-check-updates": "^17.1.11",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "prisma": "^5.8.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
